%{
open Stroodel.Syntax
%}

%token VARIABLEREFERENCE
%token ATOMTYPE
%token ATOMLITERAL

%token <string> SYMBOL

// symbolic tokens:
%token TICK
%token EOF

// the parser returns an _Expr_:
%start start
%type < Expr > start

%%

start: File EOF { $1 }

File:
	| TICK SYMBOL				{ AtomLiteral ("'" + $2) }
	| VARIABLEREFERENCE SYMBOL	{ VariableReference $2 }
